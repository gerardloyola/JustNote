<!DOCTYPE html>
<html>
  <head>
    <title>JustNote</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application' %>
    <%= javascript_include_tag 'application' %>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">    <script type="text/javascript"> window.logoURL="<%= image_url('logo.svg') %>" </script> 
    <link href="https://fonts.googleapis.com/css?family=Volkhov" rel="stylesheet">
    <script type="text/javascript"> window.logoURL="<%= image_url('logo.svg') %>" </script> 
    <script type="text/javascript"> window.compURL="<%= image_url('desktop.png') %>" </script> 
    <script type="text/javascript"> window.focusURL="<%= image_url('focus.png') %>" </script> 
    <script type="text/javascript"> window.greenURL="<%= image_url('green.jpg') %>" </script> 
    <script type="text/javascript"> window.newnoteURL="<%= image_url('new-note.png') %>" </script> 
    <%= favicon_link_tag asset_path('favicon.ico') %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
